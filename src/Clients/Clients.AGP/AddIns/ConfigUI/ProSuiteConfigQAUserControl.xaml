<UserControl x:Class="Clients.AGP.ProSuiteSolution.ConfigUI.ProSuiteConfigQAUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Clients.AGP.ProSuiteSolution.ConfigUI"
             d:DataContext="{Binding Path=local.ProSuiteConfigQAViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="350" d:DesignWidth="800">
    <Grid>
        <!--<Label Content="{Binding ServiceProviderConfigs.Count}"  HorizontalAlignment="Left" Margin="30,14,0,0" VerticalAlignment="Top" Width="301" Foreground="#FFFB0808"/>-->
        <Label Content="QA service providers" HorizontalAlignment="Left" Margin="16,15,0,0" VerticalAlignment="Top" Width="140" Foreground="#FFF0EBEB"/>
        <Label Content="QA specification providers" HorizontalAlignment="Left" Margin="18,165,0,0" VerticalAlignment="Top" Width="204" Foreground="#FFF0EBEB"/>
        <ListBox ItemsSource="{Binding ServiceProviderConfigs}" HorizontalAlignment="Left" Height="95" Margin="20,46,0,0" VerticalAlignment="Top" Width="219">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <WrapPanel>
                        <TextBlock Text="{Binding ServiceType}"/>
                    </WrapPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
        <Label Content="QA service settings" HorizontalAlignment="Left" Margin="261,41,0,0" VerticalAlignment="Top" Width="185" Foreground="White"/>
        <Label Content="Service name" HorizontalAlignment="Left" Margin="278,72,0,0" VerticalAlignment="Top" Width="84" Foreground="White"/>
        <Label Content="Connection" HorizontalAlignment="Left" Margin="278,103,0,0" VerticalAlignment="Top" Width="84" Foreground="White"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="367,75,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="198"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="367,107,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="369"/>
    </Grid>
</UserControl>
